{
  "db_name": "PostgreSQL",
  "query": "\n      UPDATE cron_jobs\n      SET\n        region = $2,\n        schedule = $3,\n        timeout_ms = $4,\n        max_retries = $5,\n        max_response_bytes = $6,\n        error = NULL\n      FROM http_requests AS req\n      WHERE cron_jobs.id = $1 AND req.id = cron_jobs.request_id\n      RETURNING\n        cron_jobs.id,\n        cron_jobs.region,\n        cron_jobs.tenant_id,\n        req.method,\n        req.url,\n        req.headers,\n        req.body,\n        cron_jobs.schedule,\n        cron_jobs.timeout_ms,\n        cron_jobs.max_retries,\n        cron_jobs.max_response_bytes,\n        cron_jobs.created_at,\n        cron_jobs.error\n      ",

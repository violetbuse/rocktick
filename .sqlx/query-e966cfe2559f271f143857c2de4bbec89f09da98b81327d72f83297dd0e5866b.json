  "query": "\n  SELECT\n    job.id as id,\n    exec.request_id as req_id,\n    exec.response_id as res_id\n  FROM scheduled_jobs job\n  JOIN job_executions exec\n    ON exec.id = job.execution_id\n  WHERE\n    job.id = $1\n  ",

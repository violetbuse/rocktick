{
  "db_name": "PostgreSQL",
  "query": "\n          SELECT workflow.*\n          FROM workflows workflow\n          WHERE\n            EXISTS (\n              SELECT 1\n              FROM workflow_executions exec\n              WHERE exec.workflow_id = workflow.id\n                AND exec.status = 'pending'\n            )\n            AND NOT EXISTS (\n              SELECT 1\n              FROM workflow_executions exec\n              WHERE exec.workflow_id = workflow.id\n                AND exec.status NOT IN ('pending', 'completed', 'failed')\n            )\n          LIMIT 1 FOR UPDATE SKIP LOCKED;\n        ",

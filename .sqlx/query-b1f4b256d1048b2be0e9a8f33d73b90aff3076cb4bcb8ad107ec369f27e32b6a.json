{
  "db_name": "PostgreSQL",
  "query": "\n      UPDATE one_off_jobs\n      SET\n        region = $2,\n        execute_at = $3,\n        timeout_ms = $4,\n        max_retries = $5,\n        max_response_bytes = $6\n      FROM one_off_jobs as job\n      JOIN http_requests AS req\n        ON req.id = job.request_id\n      WHERE job.id = $1\n      RETURNING\n        job.id,\n        job.region,\n        job.tenant_id,\n        req.id as req_id,\n        req.method,\n        req.url,\n        req.headers,\n        req.body,\n        job.execute_at,\n        job.timeout_ms,\n        job.max_retries,\n        job.max_response_bytes,\n        job.created_at,\n        job.deleted_at\n      ",

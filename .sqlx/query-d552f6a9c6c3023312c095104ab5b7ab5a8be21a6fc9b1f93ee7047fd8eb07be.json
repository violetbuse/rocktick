{
  "db_name": "PostgreSQL",
  "query": "\n      WITH\n        job AS (\n          SELECT *\n          FROM one_off_jobs\n          WHERE id = $1\n            AND ($2::text IS NULL OR tenant_id = $2)\n          FOR UPDATE\n        ),\n        req AS (\n          SELECT *\n          FROM http_requests\n          WHERE id = (SELECT request_id FROM job)\n          FOR UPDATE\n        )\n      SELECT\n        job.id,\n        job.region,\n        job.execute_at,\n        job.timeout_ms,\n        job.max_retries,\n        job.max_response_bytes,\n        req.id as req_id,\n        req.method,\n        req.url,\n        req.headers,\n        req.body\n      FROM job\n      JOIN req ON req.id = job.request_id\n      ",

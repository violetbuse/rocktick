  "query": "\n      SELECT\n        job.id,\n        job.region,\n        job.schedule,\n        job.timeout_ms,\n        job.max_retries,\n        job.max_response_bytes,\n        job.request_id as req_id\n      FROM cron_jobs as job\n      WHERE job.id = $1 AND ($2::text IS NULL OR job.tenant_id = $2)\n      FOR UPDATE\n      ",
